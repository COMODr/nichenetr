citHeader("To cite nichenetr in publications use:")

citEntry(entry = "Article",
  title        = "Linking extracellular protein signals to target genes with {nichenetr}",
  author       = personList(as.person("Robin Browaeys"),
                   as.person("Wouter Saelens")),
  journal      = "Hopefully Nature Methtods ;-)",
  year         = "Hopefully 2018",
  volume       = "40",
  number       = "3",
  pages        = "1--25",
  url          = "http://superdeporte.es,

  textVersion  =
  paste("Robin Browaeys, Wouter Saelens (2018).",
        "Linking extracellular protein signals to target genes with nichenetr",
        "Hopefully Nature Methtods.",
        "URL http://superdeporte.es)
)
